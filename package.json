{
  "name": "express-openapi-generator",
  "version": "1.1.1",
  "description": "An Express plugin that can parse an app and generate a OpenApiv3 document.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --collectCoverage --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint-fix": "eslint -c .eslintrc.js --ext .ts src --fix",
    "gen-docs": "typedoc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run gen-docs && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nklisch/express-openapi-generator.git"
  },
  "funding": {
    "type": "buyMeACoffee",
    "url": "https://www.buymeacoffee.com/nklisch"
  },
  "keywords": [
    "express",
    "openapi",
    "swagger",
    "redoc",
    "documentation",
    "generator",
    "autodocumentation",
    "tools",
    "plugin",
    "middleware",
    "code-generation",
    "validation"
  ],
  "author": "Nathan Klisch",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "bugs": {
    "url": "https://github.com/nklisch/express-openapi-generator/issues"
  },
  "homepage": "https://nklisch.github.io/express-openapi-generator/",
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/supertest": "^2.0.12",
    "@types/express": "4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "ajv-formats": "^2.1.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.30.1",
    "express": "^4.18.1",
    "generate-ts-docs": "^0.0.13",
    "jest": "^28.1.2",
    "openapi-schema-validator": "^12.0.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.9",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@types/ajv": "^1.x",
    "@types/express": "^4.x",
    "ajv": "8.x",
    "express": "^4.x",
    "openapi-types": "^12.x"
  },
  "peerDependenciesMeta": {
    "ajv": {
      "optional": true
    },
    "openapi-types": {
      "optional": true
    },
    "@types/ajv": {
      "optional": true
    },
    "@types/express": {
      "optional": true
    }
  },
  "dependencies": {
    "express-route-parser": "^1.0.4"
  }
}
