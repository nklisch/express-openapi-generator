  "author": "Nathan Klisch",
  "contributors": [
    "Wes Todd <wes@wesleytodd.com>"
  ],
  "license": "ISC",
  "files": "lib/**/*",
  "bugs": {
    "url": "https://github.com/nklisch/express-openapi-tools/issues"
  },
  "homepage": "https://github.com/nklisch/express-openapi-tools#readme",
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.30.1",
    "express": "^4.18.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "openapi-types": "^12.0.0",
    "path-to-regexp": "^6.2.1"
  }
}
